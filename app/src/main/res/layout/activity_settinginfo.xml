<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/scrollView1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundwhite"
    android:scrollbars="none" >

<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundwhite"
    android:orientation="vertical" >
    
       <RelativeLayout
	        android:id="@+id/title"
	        android:layout_width="match_parent"
	        android:layout_height="@dimen/height_top_bar"
	        android:background="@color/backgroundwhite" >
	
	        <LinearLayout
	            android:layout_width="@dimen/height_back_icon_size"
	            android:layout_height="@dimen/height_back_icon_size"
	            android:onClick="back"
	             >
	
	            <ImageView
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_gravity="center_vertical"
	                android:layout_marginLeft="@dimen/sharemarginleft"
	                android:scaleType="centerInside"
	                android:src="@drawable/back" />
	        </LinearLayout>
	
	        <TextView
	            android:id="@+id/selfname"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerInParent="true"
	            android:text="@string/setting"
	            android:textColor="@color/fontcolor"
	            android:textSize="@dimen/text_size_l" />

	    </RelativeLayout>
	    <View
             android:layout_width="fill_parent"
             android:layout_height="1dp"
             android:background="@color/backgroundsep" />
	    
	     <RelativeLayout
        android:layout_width="match_parent"
         android:layout_marginLeft="@dimen/sharemarginleft"
          android:id="@+id/modifypwd"
          android:layout_marginRight="@dimen/sharemarginleft"
        android:layout_height="@dimen/setting_content_height" >
         
	      <ImageView
               android:id="@+id/img1"
               android:layout_width="25dp"
               android:layout_height="wrap_content"
               android:layout_centerHorizontal="false"
               android:layout_centerVertical="true"
               android:layout_marginLeft="5dp"
               android:layout_marginRight="5dp"
               android:background="@drawable/spwd" />
        
        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/img1"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:textSize="@dimen/text_size_mm"
            android:text="@string/modifypwd" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:src="@drawable/setenter" />
        
         
    </RelativeLayout>
	<View
             android:layout_width="fill_parent"
             android:layout_height="1dp"
             android:background="@color/backgroundsepold" />
	
	 <RelativeLayout
        android:layout_width="match_parent"
         android:layout_marginLeft="@dimen/sharemarginleft"
          android:layout_marginRight="@dimen/sharemarginleft"
         android:id="@+id/bindphone"
        android:layout_height="@dimen/setting_content_height" >
	        
	      <ImageView
               android:id="@+id/img0"
               android:layout_width="25dp"
               android:layout_height="wrap_content"
               android:layout_centerHorizontal="false"
               android:layout_centerVertical="true"
               android:layout_marginLeft="5dp"
               android:layout_marginRight="5dp"
               android:background="@drawable/phonebig" />
        
        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/setting_content_height"
            android:layout_toRightOf="@+id/img0"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:text="@string/bindphone"
            android:textSize="@dimen/text_size_mm" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/setting_content_height"
            android:layout_alignParentRight="true"
           
            android:src="@drawable/setenter" />
        
         <View
             android:layout_width="fill_parent"
             android:layout_height="1dp"
             android:layout_alignParentBottom="true"
             android:background="@color/backgroundsepold" />

    </RelativeLayout>
    


   <View
             android:layout_width="fill_parent"
             android:layout_height="1dp"
             android:background="@color/backgroundsepold" />

 <TextView
	         android:layout_width="fill_parent"
	         android:layout_height="@dimen/buttonoptheight"
	         android:paddingLeft="10dp"
	        android:background="@color/backgroundwhite2"
	         android:gravity="center_vertical"
	         android:text="@string/about" />
      <View
             android:layout_width="fill_parent"
             android:layout_height="1dp"
             android:background="@color/backgroundsepold" />
    
     <RelativeLayout
        android:layout_width="match_parent"
        android:layout_marginLeft="@dimen/sharemarginleft"
         android:id="@+id/layoutAbout"
         android:layout_marginRight="@dimen/sharemarginleft"
        android:layout_height="@dimen/setting_content_height" >
        
          <ImageView
               android:id="@+id/img5"
               android:layout_width="25dp"
               android:layout_height="wrap_content"
               android:layout_centerHorizontal="false"
               android:layout_centerVertical="true"
               android:layout_marginLeft="5dp"
               android:layout_marginRight="5dp"
               android:background="@drawable/sabout" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/img5"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:textSize="@dimen/text_size_mm"
            android:text="@string/aboutapp" />

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            
            android:src="@drawable/setenter" />

        <View
             android:layout_width="fill_parent"
             android:layout_height="1dp"
             android:layout_alignParentBottom="true"
             android:background="@color/backgroundsepold" />
    </RelativeLayout>
    
     <RelativeLayout
        android:layout_width="match_parent"
        android:layout_marginLeft="@dimen/sharemarginleft"
        android:id="@+id/layoutClearCache"
        android:layout_marginRight="@dimen/sharemarginleft"
        android:layout_height="@dimen/setting_content_height" >
        
            <ImageView
               android:id="@+id/img6"
               android:layout_width="25dp"
               android:layout_height="wrap_content"
               android:layout_centerHorizontal="false"
               android:layout_centerVertical="true"
               android:layout_marginLeft="5dp"
               android:layout_marginRight="5dp"
               android:background="@drawable/sclearcache" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/img6"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:textSize="@dimen/text_size_mm"
            android:text="@string/clearcache" />

        <ImageView
            android:id="@+id/imageView31"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            
            android:src="@drawable/setenter" />

        <View
             android:layout_width="fill_parent"
             android:layout_height="1dp"
             android:layout_alignParentBottom="true"
             android:background="@color/backgroundsepold" />
    </RelativeLayout>
    
       <RelativeLayout
        android:layout_width="match_parent"
        android:layout_marginLeft="@dimen/sharemarginleft"
        android:id="@+id/layoutUserInfoBack"
        android:layout_marginRight="@dimen/sharemarginleft"
        android:layout_height="@dimen/setting_content_height" >
        
           <ImageView
               android:id="@+id/img7"
               android:layout_width="25dp"
               android:layout_height="wrap_content"
               android:layout_centerHorizontal="false"
               android:layout_centerVertical="true"
               android:layout_marginLeft="5dp"
               android:layout_marginRight="5dp"
               android:background="@drawable/smsbig" />
           

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/img7"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:textSize="@dimen/text_size_mm"
            android:text="@string/userinfoback" />

        <ImageView
            android:id="@+id/imageView33"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            
            android:src="@drawable/setenter" />

        <View
             android:layout_width="fill_parent"
             android:layout_height="1dp"
             android:layout_alignParentBottom="true"
             android:background="@color/backgroundsepold" />
    </RelativeLayout>
    
     
    
       <RelativeLayout
        android:layout_width="match_parent"
        android:layout_marginLeft="@dimen/sharemarginleft"
         android:id="@+id/layoutUserProxy"
         android:layout_marginRight="@dimen/sharemarginleft"
        android:layout_height="@dimen/setting_content_height" >
        
           <ImageView
               android:id="@+id/img8"
               android:layout_width="25dp"
               android:layout_height="wrap_content"
               android:layout_centerHorizontal="false"
               android:layout_centerVertical="true"
               android:layout_marginLeft="5dp"
               android:layout_marginRight="5dp"
               android:background="@drawable/suserproxy" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_toRightOf="@+id/img8"
            android:layout_centerVertical="true"
            android:gravity="center_vertical"
            android:textSize="@dimen/text_size_mm"
            android:text="@string/userproxy" />

        <ImageView
            android:id="@+id/imageView34"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            
            android:src="@drawable/setenter" />

        
    </RelativeLayout>
    
        <RelativeLayout
               android:layout_width="match_parent"
               android:layout_height="0dp"
               android:layout_marginTop="@dimen/home_page_under_button_sep"
               android:layout_marginBottom="@dimen/home_page_under_button_sep"
               android:layout_weight="1"
               >
        
                 <RelativeLayout
                     android:id="@+id/layoutLogout"
                     android:layout_width="wrap_content"
                     android:layout_height="@dimen/setting_head_size"
                     android:layout_alignParentBottom="true"
                     android:layout_centerHorizontal="true"
                     android:layout_centerInParent="false"
                     android:layout_centerVertical="true"
                      >

        <TextView
            android:id="@+id/textView32"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/sharemarginleft"
            android:layout_marginRight="@dimen/sharemarginleft"
            android:layout_centerHorizontal="true"
            android:layout_alignParentBottom="true"
            android:gravity="center_vertical|center_horizontal"
            android:background="@drawable/btn_light"
            android:text="@string/logout"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_mm" />

       

    </RelativeLayout>
           </RelativeLayout>
    </LinearLayout>
</ScrollView>