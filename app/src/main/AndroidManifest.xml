            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.DetailNoticeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.CreatTeamActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.TeamDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ui.PersonalDetailsActivity"
            android:screenOrientation="portrait" />
        <activity
